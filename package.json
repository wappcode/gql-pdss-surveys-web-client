{
  "name": "gql-pdss-surveys-web-client",
  "private": false,
  "version": "3.0.2",
  "type": "module",
  "main": "./dist/gql-pdss-surveys-web-client.umd.cjs",
  "module": "./dist/gql-pdss-surveys-web-client.js",
  "typings": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/gql-pdss-surveys-web-client.js",
